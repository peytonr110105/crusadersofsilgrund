on inventory click:
	if the name of the player's current inventory contains "Perk":
		if the name of the player's current inventory does not contain "Preview":
			if the clicked item is emerald block:
				set {perkSelected.%player%} to decolorize(name of the clicked item)
				#send formatted "%{perkSelected.%player%}%" to the player
				make player execute command "/perks 99"
			else if the name of the clicked item is "<bold>Next Page":
				make player execute command "/perks generics2"
			else if the name of the clicked item is "<bold>Previous Page":
				make player execute command "/perks 1"
			else if the name of the clicked item is "<bold>Class Perks":
				make player execute command "/perks 2"
			else if the name of the clicked item is "<bold>Return to Directory":
				make player execute command "/perks 2"
			else if the name of the clicked item is "<bold>Forma Perks":
				make player execute command "/perks forma"
			else if the name of the clicked item contains "<gold>":
				if the name of the player's current inventory contains "Directory":
					set {_class} to decolorize(name of the clicked item)
					make player execute command "/perks %{_class}%"
			else if the name of the clicked item contains "<gray>":
				if the name of the player's current inventory contains "Tree":
					#Gets the class of the tree the player is viewing.
					set {_class::*} to the name of the player's current inventory split at " Perk"
					set {_class} to decolorize({_class::1})
					send "%{_class}%" to the player
					set {_tree} to decolorize(name of the clicked item)
					set {activePerkTree.%player%::%{_class}%} to {_tree}
					make player execute command "/perks %{_class}%"
			cancel event
	else if the name of the player's current inventory contains "<gold>Select a slot to equip":
		if the clicked item is chest:
			set {_perk} to {perkSelected.%player%}
			add "<bold>" to {_perk}
			loop all items in the player's current inventory:
				if the name of the loop-item is {_perk}:
					exit 1 loop
					cancel event
					set {_p} to 1
			if {_p} is not set:
				set {_slot} to the index of event-slot
				make player execute command "/perk %{_slot} + 1% %{perkSelected.%player%}%"
				make player execute command "/perks 99"
		if the clicked item is red glass pane:
			make player execute command "/perks 1"
			clear {perkSelected.%player%}
		cancel event