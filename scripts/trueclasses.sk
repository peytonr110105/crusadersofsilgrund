#MARKER - Character Info
		
command /cstats [<offlineplayer=%player%>] [<number=1>]:
	description: Check your or someone else's classes statistics.
	usage: /cstats (player) (page)
	aliases: /cinfo, /c, /class, /seestats
	executable by: players, console
	trigger:
		if {class.%arg 1%} is not set:
			send "<red>Could not find classes data for that player. Please check your spelling and try again. The player may also have disabled classes for their character."
		else:
			send "<gold><bold>%arg 1%: Level %{lvl.%arg 1%}% %{class.%arg 1%}%"
			if arg 2 is 1:
				#Calculate BST
				set {_bstcap.%arg 1%} to ({strcap.%arg 1%} + {defcap.%arg 1%} + {sklcap.%arg 1%} + {spdcap.%arg 1%} + {rescap.%arg 1%} + {hpcap.%arg 1%})
				set {bst.%arg 1%} to ({str.%arg 1%} + {def.%arg 1%} + {skl.%arg 1%} + {spd.%arg 1%} + {res.%arg 1%} + {hp.%arg 1%})
				#HP Readout
				if {hp.%arg 1%} is equal to {hpcap.%arg 1%}:
					send "<yellow><bold>Max HP: %{hp.%arg 1%}% + %{hpbuff.%arg 1%}% | Cap: %{hpcap.%arg 1%}% | Growth Rate: %{hpgrowth.%arg 1%}%"
				else:
					send "Max HP: %{hp.%arg 1%}% + %{hpbuff.%arg 1%}% | Cap: %{hpcap.%arg 1%}% | Growth Rate: %{hpgrowth.%arg 1%}%"
				#STR Readout
				if {str.%arg 1%} is equal to {strcap.%arg 1%}:
					send "<yellow><bold>Strength: %{str.%arg 1%}% + %{strbuff.%arg 1%}% | Cap: %{strcap.%arg 1%}% | Growth Rate: %{strgrowth.%arg 1%}%"
				else:
					send "Strength: %{str.%arg 1%}% + %{strbuff.%arg 1%}% | Cap: %{strcap.%arg 1%}% | Growth Rate: %{strgrowth.%arg 1%}%"
				#DEF Readout
				if {def.%arg 1%} is equal to {defcap.%arg 1%}:
					send "<yellow><bold>Defense: %{def.%arg 1%}% + %{defbuff.%arg 1%}% | Cap: %{defcap.%arg 1%}% | Growth Rate: %{defgrowth.%arg 1%}%"
				else:
					send "Defense: %{def.%arg 1%}% + %{defbuff.%arg 1%}% | Cap: %{defcap.%arg 1%}% | Growth Rate: %{defgrowth.%arg 1%}%"
				#SKL Readout
				if {skl.%arg 1%} is equal to {sklcap.%arg 1%}:
					send "<yellow><bold>Skill: %{skl.%arg 1%}% + %{sklbuff.%arg 1%}% | Cap: %{sklcap.%arg 1%}% | Growth Rate: %{sklgrowth.%arg 1%}%"
				else:
					send "Skill: %{skl.%arg 1%}% + %{sklbuff.%arg 1%}% | Cap: %{sklcap.%arg 1%}% | Growth Rate: %{sklgrowth.%arg 1%}%"
				#SPD Readout
				if {spd.%arg 1%} is equal to {spdcap.%arg 1%}:
					send "<yellow><bold>Speed: %{spd.%arg 1%}% + %{spdbuff.%arg 1%}% | Cap: %{spdcap.%arg 1%}% | Growth Rate: %{spdgrowth.%arg 1%}%"
				else:
					send "Speed: %{spd.%arg 1%}% + %{spdbuff.%arg 1%}% | Cap: %{spdcap.%arg 1%}% | Growth Rate: %{spdgrowth.%arg 1%}%"
				#MAG Readout
				if {res.%arg 1%} is equal to {rescap.%arg 1%}:
					send "<yellow><bold>Magic: %{res.%arg 1%}% + %{resbuff.%arg 1%}% | Cap: %{rescap.%arg 1%}% | Growth Rate: %{maggrowth.%arg 1%}%"
				else:
					send "Magic: %{res.%arg 1%}% + %{resbuff.%arg 1%}% | Cap: %{rescap.%arg 1%}% | Growth Rate: %{maggrowth.%arg 1%}%"
				send "BST: %{bst.%arg 1%}% (Max: %{_bstcap.%arg 1%}%)"
				send "<run command:/cstats %arg 1% 2><gold>Next Page"
			else if arg 2 is 2:
				if {perdiff.%arg 1%} is 0:
					set {_diff} to "<green>Relaxed"
				else if {perdiff.%arg 1%} is 1:
					set {_diff} to "<light green>Easy"
				else if {perdiff.%arg 1%} is 2:
					set {_diff} to "Normal"
				else if {perdiff.%arg 1%} is 3:
					set {_diff} to "<light red>Hard"
				else if {perdiff.%arg 1%} is 4:
					set {_diff} to "<light purple>Lunatic"
				else if {perdiff.%arg 1%} is 5:
					set {_diff} to "<purple><bold>Infernal"
				else if {perdiff.%arg 1%} is 6:
					set {_diff} to "<light yellow>Thracian"
				else if {perdiff.%arg 1%} is 7:
					set {_diff} to "<light yellow><bold>Lunatic (Thracian)"
				else if {perdiff.%arg 1%} is 8:
					set {_diff} to "<gold><bold>Infernal (Thracian)"
				send "Difficulty Modifier: %{_diff}%"
				send "Times Reclassed: %{reclassed.%arg 1%}%"
				send "Blocks Mined: %{blocksmined.%arg 1%}%"
				send "Tools Broken: %{toolbreaktally.%arg 1%}%"
				send "Fall Damage Taken: %{slfalltotal.%arg 1%}%"
				send "Promotion Tier: %{promoted.%arg 1%}%"
				send "<run command:/cstats %arg 1% 3><gold>Next Page"
				send "<run command:/cstats %arg 1% 1><gold>Previous Page"
			else if arg 2 is 3:
				send "<bold>%arg 1%'s perks:"
				send "Perks Unlocked: %{skillstotal.%arg 1%}%"
				send "Perk Slot 1: %{skillslot1.%arg 1%}%"
				send "Perk Slot 2: %{skillslot2.%arg 1%}%"
				send "Perk Slot 3: %{skillslot3.%arg 1%}%"
				send "Perk Slot 4: %{skillslot4.%arg 1%}%"
				send "Perk Slot 5: %{skillslot5.%arg 1%}%"
				send "<run command:/cstats %arg 1% 4><gold>Next Page"
				send "<run command:/cstats %arg 1% 2><gold>Previous Page"
			else if arg 2 is 4:
				if {party.%arg 1%} is not set:
					send "<bold>%arg 1% is not a member of a group."
				else:
					loop {%{party.%player%}%.members::*}:
						add 1 to {_members}
					if {%{party.%arg 1%}%.needInvite} is 1:
						set {_nI} to "True"
					else:
						set {_nI} to "False"
					send "<bold>%arg 1%'s Group:"
					send "Group: %{party.%arg 1%}%"
					send "Members: %{_members}%"
					send "Invite Needed: %{_nI}%"
				send "<run command:/cstats %arg 1% 5><gold>Next Page"
				send "<run command:/cstats %arg 1% 3><gold>Previous Page"
			else if arg 2 is 5:
				send "<gold><bold>%arg 1%'s Weapon Ranks"
				send "Swords: %{weapongrade.%arg 1%::swords}% | Points: %{weaponrank.%arg 1%::swords}%"
				send "Lances: %{weapongrade.%arg 1%::lances}% | Points: %{weaponrank.%arg 1%::lances}%"
				send "Axes: %{weapongrade.%arg 1%::axes}% | Points: %{weaponrank.%arg 1%::axes}%"
				send "Bows: %{weapongrade.%arg 1%::bows}% | Points: %{weaponrank.%arg 1%::bows}%"
				send "Magic: %{weapongrade.%arg 1%::magic}% | Points: %{weaponrank.%arg 1%::magic}%"
				send "Staves: %{weapongrade.%arg 1%::staves}% | Points: %{weaponrank.%arg 1%::staves}%"
				send "<run command:/cstats %arg 1% 6><gold>Next Page"
				send "<run command:/cstats %arg 1% 4><gold>Previous Page"
			else if arg 2 is 6:
				send "<gold><bold>%arg 1%'s Misc. Records"
				send "Hours played: %{hoursplayed.%arg 1%}%"
				send "Combat Victories: %{kills.%arg 1%}%"
				send "Deaths: %{deaths.%arg 1%}%"
				send "Most Damage Dealt in One Hit: %{highestdamage.%arg 1%}%"
				send "Most Damage Taken in One Hit: %{highestsuffered.%arg 1%}%"
				send "<run command:/cstats %arg 1% 5><gold>Previous Page"
				
				
#MARKER - Promotion Items


on right-click with nether star:
	if {lvl.%player%} is more than 9:
		if the name of the event-item is "<gold><bold>Journey's Crest":
			send "<gold>You may now promote into the following classes. Please click on one to learn about that class. Click the button that appears beneath the class's information to promote."
			play sound "entity.player.levelup" with volume 2 and pitch 2 at player for player
			spawn firework at the player
			wait 1 second
			set {promotionready.%player%} to 1
			if {promoted.%player%} is 0:
				delete player's held item
				play sound "entity.experience_orb.pickup" with volume 2 and pitch 1 at player for player
				send "<run command:/class help Knight><light blue>Knight"
				wait 0.4 seconds
				play sound "entity.experience_orb.pickup" with volume 2 and pitch 1 at player for player
				send "<run command:/class help Cavalier><light blue>Cavalier"
				wait 0.4 seconds
				play sound "entity.experience_orb.pickup" with volume 2 and pitch 1 at player for player
				send "<run command:/class help Bladesman><light blue>Bladesman"
				wait 0.4 seconds
				play sound "entity.experience_orb.pickup" with volume 2 and pitch 1 at player for player
				send "<run command:/class help Fighter><light blue>Fighter"
				wait 0.4 seconds
				play sound "entity.experience_orb.pickup" with volume 2 and pitch 1 at player for player
				send "<run command:/class help Mercenary><light blue>Mercenary"
				wait 0.4 seconds
				play sound "entity.experience_orb.pickup" with volume 2 and pitch 1 at player for player
				send "<run command:/class help Cleric><light blue>Cleric"
				wait 0.4 seconds
				play sound "entity.experience_orb.pickup" with volume 2 and pitch 1 at player for player
				send "<run command:/class help Mage><light blue>Mage"
				wait 0.4 seconds
				play sound "entity.experience_orb.pickup" with volume 2 and pitch 1 at player for player
				send "<run command:/class help Archer><light blue>Archer"
				wait 0.4 seconds
				play sound "entity.experience_orb.pickup" with volume 2 and pitch 1 at player for player
				send "<run command:/class help Gunslinger><light blue>Gunslinger"
				wait 0.4 seconds
				play sound "entity.experience_orb.pickup" with volume 2 and pitch 1 at player for player
				send "<run command:/class help Devil Hunter><light blue>Devil Hunter"
				wait 0.4 seconds
				play sound "entity.experience_orb.pickup" with volume 2 and pitch 1 at player for player
				send "<run command:/class help Strategist><light blue>Strategist"
				wait 0.4 seconds
				play sound "entity.experience_orb.pickup" with volume 2 and pitch 1 at player for player
				send "<run command:/class help Survivor><light blue>Survivor"
				wait 0.4 seconds
				play sound "entity.experience_orb.pickup" with volume 2 and pitch 1 at player for player
				send "<run command:/class help Dragonborn><light blue>Dragonborn"
			else:
				send "<red>You must be a Journeyman to use a Journey's Crest."
		else if the name of the event-item is "<gold><bold>Warrior's Proof":
			if {promoted.%player%} is 1:
				delete player's held item
				set {promotionready.%player%} to 1
				send "<run command:/class promote %{class.%player%}%1><gold>You may now promote into a higher-tier version of your current class. Please click here to confirm promotion."
			else if {promoted.%player%} is 2:
				delete player's held item
				set {promotionready.%player%} to 1
				send "<run command:/class promote %{class.%player%}%><gold>You may now promote into a higher-tier version of your current class. Please click here to confirm promotion."
			else if {promoted.%player%} is more than 2:
				delete player's held item
				set {promotionready.%player%} to 1
				if {class.%player%} is "Junior Lord":
					send "<run command:/class promote Junior Lord><gold>You may now promote into a higher-tier version of your current class. Please click here to confirm promotion."
				else:
					send "<run command:/class promote Overclass><gold>You may now Overclass to increase all stat caps by 15. Click here to begin Overclassing."				
			else:
				send "<red>You must be promoted to use a Warrior's Proof."
		else if the name of the event-item is "<gold><bold>Trainee's Enrollment":
			delete player's held item
			set {promotionready.%player%} to 1
			send "<gold>You may now reclass into a Journeyman. Optionally, you may reallocate your stat points. Click YES if you would like to reallocate your stat points. Click NO to keep your allocations."
			send "<run command:/charsetup 0 1><green>YES (This will reset your stats.)"
			send "<run command:/class promote Journeyman_Reclass><red>NO (You will retain your stats.)"
	else:
		send "<red>You lack enough experience to promote. You must be at least level 10 to promote."
				
				
#MARKER - Promotion Handler


command /class <text> [<text="Journeyman">]:
	description: Learn about the available classes.
	usage: /class help
	executable by: players
	trigger:
		send "<gold><bold>~~~~~~Classes~~~~~~"
		if arg 1 is "help":
			#send "<gold>Current Class: %{class.%player%}%"
			if arg 2 is "Journeyman":
				send "Journeymen are your average traveller, skilled in all manners of combat and survival. Highly versatile and able to adapt to any situation. However, this also keeps them from excelling in any one area in particular."
				send "Class-Specific Skill: <red>None"
				send "Maximum Health: <yellow>Average"
				send "Movement Speed: <yellow>Average"
				send "Damage Resistance: <yellow>Average"
				send "Attack Power: <yellow>Average"
				send "Skill: <yellow>Average"
				set {promotiontype.%player%} to "Journeyman"
			else if arg 2 is "Knight":
				send "Knights are strong and tough. Able to absorb the mightiest of blows, Knights are perfect for defence. Their poor speed and weak retaliation keeps them off the front lines. Unable to ride mounts."
				send "Class-Specific Skill: <light blue>Unbreakable: You cannot be debuffed."
				send "Maximum Health: <green>Exceptional"
				send "Movement Speed: <red>Poor"
				send "Damage Resistance: <green>Exceptional"
				send "Attack Power: <light red>Below-Average"
				send "Skill: <light red>Below-Average"
				set {promotiontype.%player%} to "Knight"
			else if arg 2 is "Cavalier":
				send "Cavaliers are mounted horsemen with solid fundamentals of swordplay. They charge fiercely into battle upon a trusted steed. Not quite as deadly if their mount is slain."
				send "Class-Specific Skill: <light blue>Taming Mastery: All animals you tame recieve permanent stat increases."
				send "Maximum Health: <yellow>Average"
				send "Movement Speed: <yellow>Average"
				send "Damage Resistance: <yellow>Average"
				send "Attack Power: <yellow>Average"
				send "Skill: <light green>Above-Average"
				set {promotiontype.%player%} to "Cavalier"
			else if arg 2 is "Bladesman":
				send "Bladesmen have devoted their lives to the sword, delivering staggeringly fast attacks with incredible skill. However, they are incredibly fragile to physical attacks."
				send "Class-Specific Skill: <light blue>Light Feet: Dodge rolling costs nothing."
				send "Maximum Health: <light red>Below-Average"
				send "Movement Speed: <green>Exceptional"
				send "Damage Resistance: <light red>Below-Average"
				send "Attack Power: <light red>Below-Average"
				send "Skill: <green>Exceptional"
				set {promotiontype.%player%} to "Bladesman"
			else if arg 2 is "Fighter":
				send "Axemen are incredibly handy with an axe. They deliver devastating blows that can smash even the strongest of armour. However, they can be somewhat sluggish and clumsy."
				send "Class-Specific Skill: <light blue>Gamble: Grants STR +60, but attacks can miss."
				send "Maximum Health: <light green>Above-Average"
				send "Movement Speed: <light red>Below-Average"
				send "Damage Resistance: <yellow>Average"
				send "Attack Power: <light green>Above-Average"
				send "Skill: <red>Poor"
				set {promotiontype.%player%} to "Fighter"
			else if arg 2 is "Mercenary":
				send "Mercenaries are the ascended form of Journeymen. Still sharp as ever in all types of combat, the Mercenary also gains slightly more skill and physical bulk."
				send "Class-Specific Skill: <light blue>Awakening: A surge of energy increases all stats in a pinch."
				send "Maximum Health: <green>Above-Average"
				send "Movement Speed: <yellow>Average"
				send "Damage Resistance: <yellow>Average"
				send "Attack Power: <yellow>Average"
				send "Skill: <light green>Above-Average"
				set {promotiontype.%player%} to "Mercenary"
			else if arg 2 is "Archer":
				send "Archers specialize in ranged combat. Their above average defences lend themselves well to bow duels, but are weak in close-quarters combat."
				send "Class-Specific Skill: <light blue>Quick Draw: Arrows fire instantly, but travel less distance."
				send "Maximum Health: <yellow>Average"
				send "Movement Speed: <yellow>Average"
				send "Damage Resistance: <light green>Above-Average"
				send "Attack Power: <light red>Below-Average"
				send "Skill: <light red>Below-Average"
				set {promotiontype.%player%} to "Archer"
			else if arg 2 is "Strategist":
				send "Strategists are highly versatile fighters familiar with all manners of combat. They can wield any weapon necessary to secure victory for their comrades."
				send "Class-Specific Skill: <light blue>Rally Strength: Right-Click an ally to grant them Strength 1 for 60 seconds."
				send "Maximum Health: <yellow>Average"
				send "Movement Speed: <yellow>Average"
				send "Damage Resistance: <yellow>Average"
				send "Attack Power: <yellow>Average"
				send "Skill: <yellow>Average"
				set {promotiontype.%player%} to "Strategist"
			else if arg 2 is "Gunslinger":
				send "Gunslingers are fast-footed tricksters posessing superb skills with all manners of firearms. Their speed and skill make them hard to pin down, even in close quarters."
				send "Class-Specific Skill: <light blue>Fast Fingers: Halves the reload time of all firearms."
				send "Maximum Health: <yellow>Average"
				send "Movement Speed: <light green>Above-Average"
				send "Damage Resistance: <light-red>Below-Average"
				send "Attack Power: <yellow>Average"
				send "Skill: <light green>Above-Average"
				set {promotiontype.%player%} to "Gunslinger"
			else if arg 2 is "Devil Hunter":
				send "Devil Hunters are high-flying and stylish masters of both ranged and melee combat. While their defences are rather lacking, they make up for it with lightning-fast and stylish combo attacks."
				send "Class-Specific Skill: <light blue>Air Hike: Perform wall jumps by left-clicking a wall while in contact with it."
				send "Maximum Health: <red>Poor"
				send "Movement Speed: <green>Exceptional"
				send "Damage Resistance: <red>Poor"
				send "Attack Power: <light red>Below-Average"
				send "Skill: <green>Exceptional"
				set {promotiontype.%player%} to "Devil Hunter"
			else if arg 2 is "Dragonborn":
				send "A near-extinct race with the blood of dragons. They possess power beyond that of normal humans, but can only use it for a limited time."
				send "Class-Specific Skill: <light blue>Hoard: Gain bonus money equal to your MAG at the start of every new day."
				send "Maximum Health: <yellow>Average"
				send "Movement Speed: <yellow>Average"
				send "Damage Resistance: <yellow>Average"
				send "Attack Power: <green>Exceptional"
				send "Skill: <yellow>Average"
				set {promotiontype.%player%} to "Dragonborn"
			else if arg 2 is "Survivor":
				send "Survivors are focused only on their own well-being. Foregoing any more combat talent than necessary, they focus on craftsmanship and retriving resources quickly and efficiently."
				send "Class-Specific Skill: <light blue>Scavenger: Ores have a chance to drop extra ore pieces when broken."
				send "Maximum Health: <yellow>Average"
				send "Movement Speed: <yellow>Average"
				send "Damage Resistance: <yellow>Average"
				send "Attack Power: <yellow>Average"
				send "Skill: <yellow>Average"
				set {promotiontype.%player%} to "Survivor"
			else if arg 2 is "Cleric":
				send "Clerics are helpful healers that wield a variety of staves. While they may not be able to use typical weapons, the staves clerics carry can heal your wounded. Take one with you, and you'll be very glad you did."
				send "Class-Specific Skill: <light blue>Live to Serve: When healing a player, you heal the same amount of health as your patient."
				send "Maximum Health: <green>Above-Average"
				send "Movement Speed: <light red>Below-Average"
				send "Damage Resistance: <light-red>Below-Average"
				send "Attack Power: <red>Poor"
				send "Skill: <light red>Below-Average"
				send "Magic: <green>Exceptional"
				set {promotiontype.%player%} to "Cleric"
			else if arg 2 is "Mage":
				send "Mages are spellcasters that can wield a large variety of powerful magics. They can combine multiple elements to create unique effects on the world."
				send "Class-Specific Skill: <light blue>Magic Ward: Halves cooldown of any magical abilities."
				send "Maximum Health: <green>Above-Average"
				send "Movement Speed: <yellow>Average"
				send "Damage Resistance: <light-red>Below-Average"
				send "Phyiscal Power: <red>Poor"
				send "Skill: <yellow>Average"
				send "Magic: <green>Exceptional"
