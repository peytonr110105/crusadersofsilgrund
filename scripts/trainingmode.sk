function simulateSpecial(PLR: player, PCT: number, CAP: number) :: number:
	set {_specialOdds} to floor((({skl.%{_PLR}%} + {sklbuff.%{_PLR}%}) * {_PCT}) ^ 0.95)
	if {_specialOdds} is more than {_CAP}:
		set {_specialOdds} to {_CAP}
	return {_specialOdds}
	
function simulateSpecialOvercharge(PLR: player, CSH: number, EXP: number, CAP: number) :: number:
	set {_specialOddsOvercharge} to ({sklcap.%{_PLR}%} ^ {_EXP})
	if {_specialOddsOvercharge} is more than {_CAP}:
		set {_specialOddsOvercharge} to {_CAP}
	if the 6th line of the lore of slot 17 of {_PLR}'s current inventory contains "Slaying":
		add 10 to {_specialOddsOvercharge}
	if the name of slot 17 of {_PLR}'s current inventory is "<gold><bold>Rex Hasta":
		if the 6th line of the lore of slot 17 of {_PLR}'s current inventory contains "Unique":
			add 20 to {_specialOddsOvercharge}
	else if the name of slot 17 of {_PLR}'s current inventory is "<gold><bold>Hauteclere":
		if the 6th line of the lore of slot 17 of {_PLR}'s current inventory contains "Unique":
			add 20 to {_specialOddsOvercharge}
	if the 2nd line of the lore of slot 17 of {_PLR}'s current inventory contains "Wrath":
		add ({_PLR}'s maximum health / 2) to {_specialOddsOvercharge}
	add {_CSH} to {_specialOddsOvercharge}
	return {_specialOddsOvercharge}
			
