command /salindrum:
	aliases: /void, /training, /testroom, /sd
	trigger:
		if {sd.rewarp.%player%} is set:
			open chest inventory with 2 rows named "<red>Salindrum Configuration" for the player
			set slot 0 of the player's current inventory to 1 nether star named "<gold>Exit Salindrum" with lore "<white>Click to exit the training mode."
			set slot 1 of the player's current inventory to 1 zombie spawn egg named "<white>Spawn Monsters" with lore "<white>Generate a variety of monsters to practice with."
			set slot 2 of the player's current inventory to 1 iron sword named "<white>Test Skills" with lore "<white>Equip any unlocked skills for free."
			set slot 3 of the player's current inventory to 1 diamond sword named "<white>Test Sacred Treasures" with lore "<white>Try out one of the Sacred Treasures."
			set slot 8 of the player's current inventory to 1 tnt named "<white>Reset Session" with lore "<white>Resets the session back to when you entered."
			set slot 7 of the player's current inventory to 1 totem of undying named "<white>Damage Modifiers" with lore "<white>Fine-tune how much damage you take from and deal to foes."
			set slot 4 of the player's current inventory to 1 anvil named "<white>Fix Equipment" with lore "<white>Click to repair any damaged items in your inventory."
			set slot 5 of the player's current inventory to 1 chest named "<white>Supply Crate" with lore "<white>Borrow weapons and ammo here."
			if {sd.nohunger.%player%} is true:
				set slot 6 of the player's current inventory to 1 steak named "<white>Infinite Hunger: ON" with lore "<white>Your hunger bar won't decrease.", "<white>Click to toggle."
			else:
				set slot 6 of the player's current inventory to 1 rotten flesh named "<white>Infinite Hunger: OFF" with lore "<white>Your hunger bar will decrease normally.", "<white>Click to toggle."
			set slot 9 of the player's current inventory to 1 workbench named "<white>Combat Calculator" with lore "<white>Run a variety of calculations on your weapons."
			if {sd.infabilities.%player%} is true:
				set slot 10 of the player's current inventory to 1 iron chestplate named "<white>Max Abilities: ON" with lore "<white>You can wield any weapon with no penalty.", "<white>Click to toggle."
			else:
				set slot 10 of the player's current inventory to 1 iron chestplate named "<white>Max Abilities: OFF" with lore "<white>Your weapon abilities are unchanged.", "<white>Click to toggle."
		else:
			open chest inventory with 2 rows named "<red>Select a room." for the player
			set {_roomnum} to 0
			loop {sd::*}:
				add 1 to {_roomnum}
				if loop-value is not 0:
					set slot ({_roomnum} - 1) of the player's current inventory to 1 redstone block named "<red>Room %{_roomnum}%" with lore "<white>Occupant: %{sd::Room %{_roomnum}%}%"
				else:
					set slot ({_roomnum} - 1) of the player's current inventory to 1 lapis block named "Room %{_roomnum}%" with lore "<white>Empty Room"
				
