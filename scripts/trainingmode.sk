#
command /salindrum:
	aliases: /void, /training, /testroom, /sd
	trigger:
		if {sd.rewarp.%player%} is set:
			open chest with 1 row named "<red>Salindrum Configuration" for the player
			set slot 0 of the player's current inventory to 1 nether star named "<gold>Exit Salindrum"
			set slot 1 of the player's current inventory to 1 zombie spawn egg named "<white>Spawn Monsters"
			set slot 2 of the player's current inventory to 1 iron sword named "<white>Test Skills"
			if {quests.%player%::Awakening} is 999:
				set slot 3 of the player's current inventory to 1 diamond sword named "<white>Test Holy Weapons"
			set slot 8 of the player's current inventory to 1 tnt named "<white>Reset Session"
			if {sd.invuln.%player%} is true:
				set slot 7 of the player's current inventory to 1 totem of undying named "<white>Player Invincibility: ON" with lore "<white>You won't take damage from enemies.", "<white>Click to toggle."
			else:
				set slot 7 of the player's current inventory to 1 redstone block named "<white>Player Invincibility: OFF" with lore "<white>You will take damage from enemies.", "<white>Click to toggle."
			if {sd.nohunger.%player%} is true:
				set slot 6 of the player's current inventory to 1 steak named "<white>Infinite Hunger: ON" with lore "<white>Your hunger bar won't decrease.", "<white>Click to toggle."
			else:
				set slot 6 of the player's current inventory to 1 rotten flesh named "<white>Infinite Hunger: OFF" with lore "<white>Your hunger bar will decrease normally.", "<white>Click to toggle."
		else:
			open chest with 2 rows named "<red>Select a room." for the player
			set {_roomnum} to 0
			loop {sd::*}:
				add 1 to {_roomnum}
				if loop-value is not 0:
					set slot ({_roomnum} - 1) of the player's current inventory to 1 redstone block named "<red>Room %{_roomnum}%" with lore "<white>Occupant: %{sd::Room %{_roomnum}%}%"
				else:
					set slot ({_roomnum} - 1) of the player's current inventory to 1 lapis block named "Room %{_roomnum}%" with lore "<white>Empty Room"
				
command /sdsetup <number> <number> <number> <number>:
	trigger:
		set {sd.room %arg 1%} to location at (arg 2, arg 3, arg 4) in world "Salindrum"
		set {sd::room %arg 1%} to 0
		send "<light green>Set Room %arg 1% coordinates to %arg 2%, %arg 3%, %arg 4%."
		
