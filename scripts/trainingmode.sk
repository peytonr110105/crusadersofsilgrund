on spawn of a slime:
	if entity's world is "Salindrum":
		cancel event

on death of a monster:
	if victim's world is "Salindrum":
		wait 1 tick
		loop dropped items in radius 1 of the victim:
			kill the loop-entity

on experience spawn:
	if world is "Salindrum":
		cancel event
		
on food bar change:
	if {sd.nohunger.%player%} is true:
		increase the player's food level by 10
		
on inventory click:
	#Room Selector
	if the name of the player's current inventory is "<red>Select a room.":
		cancel event
		if the clicked item is lapis block:
			set {sd.rewarp.%player%} to location of block at player
			set {sdroom.%player%} to uncolored name of the clicked item
			set {sdcash.%player%} to player's balance
			set {sd.outgoingmult.%player%} to 1
			set {sd.incomingmult.%player%} to 1
			send "%{sdcash.%player%}%"
			add 100000 to the player's balance
			teleport the player to {sd.%{sdroom.%player%}%}
			set {sd::%{sdroom.%player%}%} to player's tablist name
			send "<light blue>Entered Salindrum."
			make player execute command "/salindrum"
			loop all items in the player's inventory:
				add loop-item to {sditems.%player%::*}
	#General Config Menu
	else if the name of the player's current inventory is "<red>Salindrum Configuration":
		cancel event
		if the name of the clicked item is "<gold>Exit Salindrum":
			teleport the player to {sd.rewarp.%player%}
			set the player's balance to {sdcash.%player%}
			clear {sdcash.%player%}
			send "%player's balance%"
			loop all monsters in radius 50 of the player:
				kill the loop-monster
			loop all items in the player's inventory:
				remove loop-item from the player
			set {_item} to 0
			loop {sditems.%player%::*}:
				if loop-value is helmet:
					set the player's helmet to loop-value
				else if loop-value is chestplate:
					set the player's chestplate to loop-value
				else if loop-value is leggings:
					set player's leggings to loop-value
				else if loop-value is boots:
					set player's boots to loop-value
				else if player has enough space for loop-value:
					add loop-value to the player's inventory
				else:
					drop loop-value at the player
			clear {sd.nohunger.%player%}
			clear {sd.invuln.%player%}
			clear {sditems.%player%::*}
			clear {sd.rewarp.%player%}
			set {sd::%{sdroom.%player%}%} to 0
			if {sd.infabilities.%player%} is set:
				clear {sd.infabilities.%player%}
				set {abilities.%player%::Traditional Weapons} to {sd_abilityBackup.%player%::Traditional Weapons}
				set {abilities.%player%::Magical Weapons} to {sd_abilityBackup.%player%::Magical Weapons}
				set {abilities.%player%::Guns} to {sd_abilityBackup.%player%::Guns}
			send "<light blue>Exited Salindrum."
		else if the name of the clicked item contains "Infinite Hunger":
			if {sd.nohunger.%player%} is not set:
				set {sd.nohunger.%player%} to true
			else:
				clear {sd.nohunger.%player%}
			make player execute command "/sd"
		else if the name of the clicked item contains "Max Abilities":
			if {sd.infabilities.%player%} is not set:
				set {sd.infabilities.%player%} to true
				set {sd_abilityBackup.%player%::Traditional Weapons} to {abilities.%player%::Traditional Weapons}
				set {sd_abilityBackup.%player%::Magical Weapons} to {abilities.%player%::Magical Weapons}
				set {sd_abilityBackup.%player%::Guns} to {abilities.%player%::Guns}
				set {abilities.%player%::Traditional Weapons} to 999
				set {abilities.%player%::Magical Weapons} to 999
				set {abilities.%player%::Guns} to 999
			else:
				clear {sd.infabilities.%player%}
				set {abilities.%player%::Traditional Weapons} to {sd_abilityBackup.%player%::Traditional Weapons}
				set {abilities.%player%::Magical Weapons} to {sd_abilityBackup.%player%::Magical Weapons}
				set {abilities.%player%::Guns} to {sd_abilityBackup.%player%::Guns}
			make player execute command "/sd"
		else if the name of the clicked item contains "Damage Modifiers":
			make player execute command "/sdmults"
		else if the name of the clicked item contains "Fix Equipment":
			make player execute command "/fix all"
		else if the name of the clicked item contains "Supply Crate":
			make player execute command "/sdsupplies"
		else if the name of the clicked item is "<white>Spawn Monsters":
			make player execute command "/sdmonsters"
		else if the name of the clicked item is "<white>Test Skills":
			make player execute command "/buyToolSkills 0 0"
		else if the name of the clicked item is "<white>Test Sacred Treasures":
			make player execute command "/sdhw 9"
		else if the name of the clicked item is "<white>Combat Calculator":
			make player execute command "/sdcalcs"
		else if the name of the clicked item is "<white>Reset Session":
			loop all monsters in radius 50 of the player:
				kill the loop-monster
			loop all items in the player's inventory:
				remove loop-item from the player
			set {_item} to 0
			loop {sditems.%player%::*}:
				if loop-value is helmet:
					set the player's helmet to loop-value
				else if loop-value is chestplate:
					set the player's chestplate to loop-value
				else if loop-value is leggings:
					set player's leggings to loop-value
				else if loop-value is boots:
					set player's boots to loop-value
				else if player has enough space for loop-value:
					add loop-value to the player's inventory
				else:
					drop loop-value at the player
			teleport the player to {sd.%{sdroom.%player%}%}
	#Sacred Treasures
	else if the name of the player's current inventory is "<red><bold>Salindrum - Sacred Treasures":
		cancel event
		if the name of the clicked item contains "<gold><bold>":
			give clicked item to the player
	#Sacred Treasures Weapon Type Select
	else if the name of the player's current inventory is "<red><bold>Salindrum - Select Weapon Class":
		cancel event
		if the name of the clicked item contains "Swords":
			make player execute command "/sdhw 0"
		else if the name of the clicked item contains "Axes":
			make player execute command "/sdhw 1"
		else if the name of the clicked item contains "Lances":
			make player execute command "/sdhw 2"
		else if the name of the clicked item contains "Bows":
			make player execute command "/sdhw 3"
		else if the name of the clicked item contains "Gauntlets":
			make player execute command "/sdhw 4"
		else if the name of the clicked item contains "Daggers":
			make player execute command "/sdhw 5"
	#Damage Modifiers
	else if the name of the player's current inventory is "<red>Damage Modifiers":
		cancel event
		if the name of the clicked item is "<light green>Increase Damage Dealt by 0.1x":
			add 0.1 to {sd.outgoingmult.%player%}
			play sound "ui.button.click" with volume 1 and pitch 1 at the player for the player
			set slot 4 of the player's current inventory to 1 iron sword named "<light green>Damage Dealt" with lore "<white>Current Modifier: %{sd.outgoingmult.%player%}%x"
		else if the name of the clicked item is "<light green>Increase Damage Taken by 0.1x":
			add 0.1 to {sd.incomingmult.%player%}
			play sound "ui.button.click" with volume 1 and pitch 1 at the player for the player
			set slot 13 of the player's current inventory to 1 iron chestplate named "<light red>Damage Taken" with lore "<white>Current Modifier: %{sd.incomingmult.%player%}%x"
		else if the name of the clicked item is "<light red>Decrease Damage Dealt by 0.1x":
			subtract 0.1 from {sd.outgoingmult.%player%}
			play sound "ui.button.click" with volume 1 and pitch 1 at the player for the player
			set slot 4 of the player's current inventory to 1 iron sword named "<light green>Damage Dealt" with lore "<white>Current Modifier: %{sd.outgoingmult.%player%}%x"
		else if the name of the clicked item is "<light red>Decrease Damage Taken by 0.1x":
			subtract 0.1 from {sd.incomingmult.%player%}
			play sound "ui.button.click" with volume 1 and pitch 1 at the player for the player
			set slot 13 of the player's current inventory to 1 iron chestplate named "<light red>Damage Taken" with lore "<white>Current Modifier: %{sd.incomingmult.%player%}%x"
		else if the name of the clicked item is "<light yellow>Reset Damage Taken Multiplier":
			set {sd.incomingmult.%player%} to 1
			play sound "ui.button.click" with volume 1 and pitch 1 at the player for the player
			set slot 13 of the player's current inventory to 1 iron chestplate named "<light red>Damage Taken" with lore "<white>Current Modifier: %{sd.incomingmult.%player%}%x"
		else if the name of the clicked item is "<light yellow>Reset Damage Dealt Multiplier":
			set {sd.outgoingmult.%player%} to 1
			play sound "ui.button.click" with volume 1 and pitch 1 at the player for the player
			set slot 4 of the player's current inventory to 1 iron sword named "<light green>Damage Dealt" with lore "<white>Current Modifier: %{sd.outgoingmult.%player%}%x"
		else if the name of the clicked item is "<bold>Go Back":
			make player execute command "/sd"
	#Supply Crate
	else if the name of the player's current inventory is "<red>Supply Crate":
		if the name of the clicked item contains "Go Back":
			cancel event
			make player execute command "/sd"
		else if the name of the clicked item contains "Restock Supply Crate":
			cancel event
			make player execute command "/sdsupplies"
	#Calculator
	else if the name of the player's current inventory is "<red>Calculator":
		if the name of the clicked item contains "Go Back":
			cancel event
			make player execute command "/sd"
		else if the name of the clicked item is "<gold>Special Odds":
			cancel event
		else if the name of the clicked item is "<gold>Damage Output":
			cancel event
		else if the name of the clicked item is "<gold>How to Use":
			cancel event
		else if the name of the clicked item is "<gold>Weapon Special Odds":
			cancel event
		else if the name of the clicked item is "<gold>Calculate":
			cancel event
			make player execute command "/runBattleSimulation"
			set slot 4 of the player's current inventory to 1 purple wool named "<gold>Special Odds" with lore "<white>Special Skill: %{sd.calcSpecial.%player%}%", "<white>Base Activation Chance: %{sd.calcOdds.%player%}%", "<white>Highest Possible Chance: %{sd.calcMaxOdds.%player%}%", "<white>SKL Needed To Cap: %{sd.calcMaxSKL.%player%}%", "<white>Critical Chance: %{sd.calcCritChance.%player%}%", "<white>Max Unbuffed Crit Chance: %{sd.calcMaxCritChance.%player%}%"
			set slot 5 of the player's current inventory to 1 splash potion of harming named "<gold>Damage Output" with lore "<white>Base Damage: %{sd.calcBaseDamage.%player%}%", "<white>With Skills & No Special: %{sd.calcSkillsDamage.%player%}%", "<white>With Skills & Special: %{sd.calcSpecialDamage.%player%}%", "<white>Maximum Damage Possible (With Double): %{sd.calcMaxDamage.%player%}%", "<white>Can Double Foes Below: %{sd.calcPlayerDoubleRange.%player%}% SPD", "<white>Foes Double From: %{sd.calcFoeDoubleRange.%player%}%+ SPD"
			if {sd.calcWeaponSpecial.%player%} is set:
				set slot 14 of the player's current inventory to 1 iron block named "<gold>Weapon Special Odds" with lore "<white>Special Skill: %{sd.calcWeaponSpecial.%player%}%", "<white>Base Activation Chance: %{sd.calcWeaponOdds.%player%}%", "<white>Highest Possible Chance: %{sd.calcWeaponMaxOdds.%player%}%", "<white>SKL Needed To Cap: %{sd.calcMAXWeaponSKL.%player%}%"
			else:
				set slot 14 of the player's current inventory to air
		else if slot 17 of the player's current inventory is not the clicked item:
			if the clicked item is not air:
				cancel event
				set slot 17 of the player's current inventory to the clicked item
				remove the clicked item from the player
		else if clicked item is not air:
			cancel event
			give clicked item to the player
			remove the clicked item from the player's current inventory
	#Spawn Monsters
	else if the name of the player's current inventory is "<red>Spawn Monsters":
		cancel event
		if the name of the clicked item is "<white>Salindrum Menu":
			make player execute command "/salindrum"
		else if the name of the clicked item is "<light red>Level Scaling":
			set {sd.dolevelscaling.%player%} to true
			set slot 52 of the player's current inventory to 1 lapis block named "<light blue>Level Scaling" with lore "<white>Monsters summoned will scale to your level.", "<white>Click to disable."
		else if the name of the clicked item is "<light blue>Level Scaling":
			clear {sd.dolevelscaling.%player%}
			set slot 52 of the player's current inventory to 1 redstone block named "<light red>Level Scaling" with lore "<white>Monsters summoned will NOT scale to your level.", "<white>Click to enable."
		else if the name of the clicked item is "<white>Zombie":
			spawn 1 zombie at {sd.%{sdroom.%player%}%}
			set {_generic} to true
		else if the name of the clicked item is "<white>Skeleton":
			spawn 1 skeleton at {sd.%{sdroom.%player%}%}
			set {_generic} to true
		else if the name of the clicked item is "<white>Spider":
			spawn 1 spider at {sd.%{sdroom.%player%}%}
			set {_generic} to true
		else if the name of the clicked item is "<white>Creeper":
			spawn 1 creeper at {sd.%{sdroom.%player%}%}
			set {_generic} to true
		else if the name of the clicked item is "<white>Husk":
			spawn 1 husk at {sd.%{sdroom.%player%}%}
			set {_generic} to true
