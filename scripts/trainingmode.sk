command /sdmults:
	trigger:
		open chest inventory with 2 rows named "<red>Damage Modifiers" for the player
		set slot 4 of the player's current inventory to 1 iron sword named "<light green>Damage Dealt" with lore "<white>Current Modifier: %{sd.outgoingmult.%player%}%x"
		set slot 3 of the player's current inventory to 1 red glass pane named "<light red>Decrease Damage Dealt by 0.1x"
		set slot 5 of the player's current inventory to 1 light green glass pane named "<light green>Increase Damage Dealt by 0.1x"
		set slot 13 of the player's current inventory to 1 iron chestplate named "<light red>Damage Taken" with lore "<white>Current Modifier: %{sd.incomingmult.%player%}%x"
		set slot 12 of the player's current inventory to 1 red glass pane named "<light red>Decrease Damage Taken by 0.1x"
		set slot 14 of the player's current inventory to 1 light green glass pane named "<light green>Increase Damage Taken by 0.1x"
		set slot 17 of the player's current inventory to 1 hopper named "<light yellow>Reset Damage Taken Multiplier"
		set slot 8 of the player's current inventory to 1 hopper named "<light yellow>Reset Damage Dealt Multiplier"
		set slot 0 of the player's current inventory to 1 red glass pane named "<bold>Go Back"
		set slot 9 of the player's current inventory to 1 red glass pane named "<bold>Go Back"

command /sdcalcs:
	trigger:
		open chest inventory with 2 rows named "<red>Calculator" for the player
		set slot 0 of the player's current inventory to 1 red glass pane named "<bold>Go Back"
		set slot 9 of the player's current inventory to 1 red glass pane named "<bold>Go Back"
		set slot 4 of the player's current inventory to 1 purple wool named "<gold>Special Odds" with lore "<white>Special Skill: %{sd.calcSpecial.%player%}%", "<white>Base Activation Chance: %{sd.calcOdds.%player%}%", "<white>Highest Possible Chance: %{sd.calcMaxOdds.%player%}%", "<white>SKL Needed To Cap: %{sd.calcMaxSKL.%player%}%", "<white>Critical Chance: %{sd.calcCritChance.%player%}%", "<white>Max Unbuffed Crit Chance: %{sd.calcMaxCritChance.%player%}%"
		set slot 5 of the player's current inventory to 1 splash potion of harming named "<gold>Damage Output" with lore "<white>Base Damage: %{sd.calcBaseDamage.%player%}%", "<white>With Skills & No Special: %{sd.calcSkillsDamage.%player%}%", "<white>With Skills & Special: %{sd.calcSpecialDamage.%player%}%", "<white>Maximum Damage Possible (With Double): %{sd.calcMaxDamage.%player%}%", "<white>Can Double Foes Below: %{sd.calcPlayerDoubleRange.%player%}% SPD", "<white>Foes Double From: %{sd.calcFoeDoubleRange.%player%}%+ SPD"
		set slot 13 of the player's current inventory to 1 book named "<gold>How to Use" with lore "<white>Click a weapon to add it to the calculator.", "<white>Click the "Calculate" button to view detailed info on your weapon.", "<white>Damage assumes foe has no equipment, and is a player.", "<white>Bows are assumed to deal a base damage of 3.5."
		set slot 8 of the player's current inventory to 1 workbench named "<gold>Calculate" with lore "<white>Click to run calculations on the selected weapon."
command /sdsetup <number> <number> <number> <number>:
	trigger:
		set {sd.room %arg 1%} to location at (arg 2, arg 3, arg 4) in world "Salindrum"
		set {sd::room %arg 1%} to 0
		send "<light green>Set Room %arg 1% coordinates to %arg 2%, %arg 3%, %arg 4%."
		
command /sdmonsters:
